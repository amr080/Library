<script>

   $(window).scroll(function () {
        var height = $(window).scrollTop();
        if (height > 1200) {
            $('#back-to-top-button').fadeIn();
        } else {
            $('#back-to-top-button').fadeOut('fast');
        };
            if (height > 100) {
            
                $('#kinexys-navbar').addClass("active");
        }  ;
        
        
         if (height < 40) {
            
                $('#kinexys-navbar').removeClass("active");
        }
    });

</script>

